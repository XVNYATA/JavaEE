<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hibernate.test.typeoverride">

    	<!-- select f from HusBand f inner join f.childrens c 
		 group by f.id having count(*) <= ? order by f.id desc  -->
    
   <query name="query_HQL1">
         select f from HusBand f inner join f.childrens c 
		 group by f.id having count(*) &lt;= ? order by f.id desc
   </query>

</hibernate-mapping>
