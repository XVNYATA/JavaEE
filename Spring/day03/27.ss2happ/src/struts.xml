<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="default" namespace="/" extends="struts-default">

		<action name="login" class="org.fkjava.action.UserAction"
		method="login">
			<result>/addBook.jsp</result>
		</action>
		
		<action name="addBook" class="org.fkjava.action.BookAction"
		method="addBook">
			<result type="chain">findAll</result>
		</action>
		
		<action name="findAll" class="org.fkjava.action.BookAction"
		method="findAll">
			<result >/success.jsp</result>
		</action>
       
    </package>

  

</struts>
